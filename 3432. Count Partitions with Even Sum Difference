function countPartitions(nums: number[]): number {

    const arr = [];
    let sum = 0;

    for(let i=0; i < nums.length; i++){
        arr.push(nums[i]+sum);
        sum+=nums[i];
    }
    let ans= 0;
    for(let i = 0;i<arr.length-1;i++){
        if( (arr[i] - arr[arr.length-1] + arr[i] ) % 2 == 0) ans++;
    }
    return ans;
};
